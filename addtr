# Fungsi Animasi Loading
loading() {
    bar="â– â–¡â–¡â–¡â–¡â–¡â–¡â–¡â–¡â–¡"
    step=1
    while [ $step -le 3 ]; do
        case $step in
            1) bar="â– â–¡â–¡â–¡â–¡â–¡â–¡â–¡â–¡â–¡" ;;
            2) bar="â– â– â– â– â–¡â–¡â–¡â–¡â–¡â–¡" ;;
            3) bar="â– â– â– â– â– â– â– â– â– â– " ;;
        esac
        echo -ne "\râ³ ${YELLOW}$1${NC}   [$bar]"
        step=$((step+1))
        sleep 1
    done
    echo -ne "\râœ… ${GREEN}$1 Selesai!${NC}          \n"
}

# Validasi Script
